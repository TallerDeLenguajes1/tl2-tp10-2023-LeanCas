@model tp10_tl2.Models.LogueoViewModel
@{
    ViewData["Title"] = "Login";
}

<h1 class="text-center">Logueo</h1>
<form asp-controller="Logueo">
    <div class="mb-3">
        <label for="usuario" class="form-label">Usuario</label>
        <input type="text" class="form-control" id="usuario" aria-describedby="usuarioHelp">
        <div id="usuarioHelp" class="form-text">Ingresa tu nombre de usuario</div>
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1">
    </div>
    <button type="submit" class="btn btn-primary">Ingresar</button>
</form>